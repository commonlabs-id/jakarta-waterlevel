{
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["react", "react-native"],
  "rules": {
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off"
  }
}
